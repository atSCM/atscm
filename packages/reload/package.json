{
  "name": "@atscm/reload",
  "version": "0.0.0",
  "description": "A proxy used to reload a browser window as changes occur",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atSCM/atscm.git",
    "directory": "packages/reload"
  },
  "author": "Lukas Hechenberger <npm@ls-age.com> (https://ls-age.com/)",
  "homepage": "https://github.com/atSCM/atscm/tree/master/packages/reload#readme",
  "bugs": {
    "url": "https://github.com/atSCM/atscm/issues"
  },
  "main": "out/server.js",
  "bin": {
    "reload": "out/bin.js"
  },
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -cw",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "reload",
    "livereload",
    "browser",
    "sync",
    "change",
    "watch",
    "browser-sync"
  ],
  "dependencies": {
    "express": "4.17.1",
    "http-proxy": "1.18.1",
    "node-fetch": "2.6.1",
    "ua-parser-js": "0.7.24"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "11.2.0",
    "@types/express": "4.17.11",
    "@types/http-proxy": "1.17.5",
    "@types/node": "14.14.31",
    "@types/node-fetch": "2.5.8",
    "esbuild": "0.8.54",
    "reconnecting-eventsource": "1.1.0",
    "rollup": "2.40.0",
    "rollup-plugin-add-shebang": "0.3.1",
    "rollup-plugin-esbuild": "2.6.1",
    "typescript": "4.2.2"
  },
  "types": "out/types/server.d.ts"
}
