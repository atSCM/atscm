{
  "source": "./src",
  "destination": "./docs/api",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "atscm-cli"
        },
        "accessor": { "access": ["public", "package", "private"] },
        "manual": {
          "files": [
            "./docs/manual/usage/CLI.md",
            "./docs/manual/usage/API.md",
            "./.github/CONTRIBUTING.md",
            "./CHANGELOG.md"
          ]
        },
        "test": {
          "source": "./test"
        }
      }
    }
  ]
}
