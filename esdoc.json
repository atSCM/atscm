{
  "source": "./src",
  "destination": "./docs/api",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "atscm"
        },
        "accessor": { "access": ["public", "package", "private"] },
        "manual": {
          "files": [
            "./docs/manual/advanced/id-conflicts.md",
            "./docs/manual/advanced/gulp-plugins.md",
            "./docs/manual/advanced/debugging.md",
            "./docs/manual/advanced/error-handling.md",
            "./docs/manual/tutorial/custom-transformer.md",
            "./docs/manual/contibute/testing.md",
            "./.github/CONTRIBUTING.md",
            "./CHANGELOG.md"
          ]
        },
        "test": {
          "source": "./test"
        }
      }
    },
    {
      "name": "@atscm/esdoc-typescript-plugin"
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": { "all": true }
    }
  ]
}
